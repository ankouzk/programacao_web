Algoritmo "Exercicio10"

Var
 num1, num2, num3, Inteiro;
  media: Real;

 Inicio
       Escreva("Digite o primeiro Número: ")
       leia(num1)
       Escreva("Digite o segundo Número: ")
       Leia(num2)
       Escreva("Digite o terceiro Número: ")
       Leia(num3)


       media <- (num1 + num2 + num3) / 3

       Escreva("A média dos números é: ", media)
       FimAlgoritmo
